{
    "python.testing.unittestArgs": [
        "-v",
        "-s",
        ".",
        "-p",
        "test_*.py"
    ],
    "python.testing.pytestEnabled": false,
    "python.testing.unittestEnabled": true,
    "python.languageServer": "Jedi",
    "autoDocstring.docstringFormat": "sphinx",
    "cSpell.words": [
        "annot",
        "pseudoname",
        "scorable",
        "sdbs"
    ],
    "autoDocstring.startOnNewLine": true,
    "pylint.args": [
        "--disable=C0415",
        "--disable=E0401",
        "--disable=W1203"
    ]
}